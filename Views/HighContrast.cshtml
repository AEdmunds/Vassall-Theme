@using (Html.BeginForm("SetContrast", "Accessability", FormMethod.Post, new{ area = "BizStrap.1.3", id="set-contrast-form" })) {
    <fieldset>
        <input id="black-on-white" name="submitButton" type="submit" value="Black on white" class="btn btn-contrast"/>
        <input id="black-on-yellow" name="submitButton" type="submit" value="Black on yellow" class="btn btn-contrast" />
        <input id="black-on-blue" name="submitButton" type="submit" value="Black on blue" class="btn btn-contrast"/>
        <input id="black-on-pink" name="submitButton" type="submit" value="Black on pink" class="btn btn-contrast"/>
    </fieldset>
    <fieldset>
        <input id="black-on-cream" name="submitButton" type="submit" value="Black on cream" class="btn btn-contrast"/>
        <input id="white-on-black" name="submitButton" type="submit" value="White on black" class="btn btn-contrast"/>
        <input id="yellow-on-black" name="submitButton" type="submit" value="Yellow on black" class="btn btn-contrast"/>
        <input id="reset-contrast" name="submitButton" type="submit" value="reset-contrast" class="btn btn-contrast"/>
    </fieldset>
} 

